[Desktop Entry]
Type=Application
Name=Import @{license.description}
Comment=Import @{license.description}
MimeType=application/x-@{license.extension};@{license.mimetype}
Exec=filebot --license %F
Icon=filebot
NoDisplay=true
StartupNotify=true
StartupWMClass=@{deb.application.class}
